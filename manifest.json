{
  "manifest_version": 2,
  "name": "Go to main page",
  "version": "1.2",
  "description": "Home page current site",
  "icons": {
    "128": "ico/128.png",
    "64": "ico/64.png"
  },
  "background": {
    "scripts": ["app.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "ico/128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "MacCtrl+Shift+H"
      }
    }
  },
  "permissions": [
    "activeTab"
  ]
}